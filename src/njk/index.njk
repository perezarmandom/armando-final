{% extends '_layouts/base.njk' %}

{% block page_title %}New index page title{% endblock %}

{% block page_description %}The search description for this page goes here.{% endblock %}

{% block content %}
<div class="container">
  <div class="row">
    <div class="col-sm-4">
      <h1>Queen</h1>
      <h2>Members</h2>
        {% for member in data.members %}
        <h3 class="entry-headline"><a href="{{ member.link }}">{{ member.name }}</a></h3>
        {% endfor %}
      <h2>Discography</h2>
        {% for album in data.discography %}
        <h3 class="entry-headline"><a href="{{ album.link }}">{{ album.name }}</a></h3>
        {% endfor %}
    </div>
  </div>
</div>
{% endblock %}
